require 'sinatra/base'

module Sinatra
  module <%= app_name.classify %>
    module Resources
      module <%= @representation.name.classify %>
        def self.registered(app)
          app.<%= (@representation.http_method || "GET").downcase %> '/<%= @representation.url_template %>' do
            # TODO replace with your logic returning <%= @representation.return_type.split(':').first %>
            "params: #{params}"
          end
        end
      end
    end
  end
  register Sinatra::<%= app_name.classify%>::Resources::<%= @representation.name.classify %>
end
